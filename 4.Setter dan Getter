using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SetterandGetter
{
    class User
    {
        static void Main(string[] args)
        {
            user dian = new user();
            dian.Username = "dian";
            dian.Password = "kopiJava";

            Console.WriteLine("Username = " + dian.Username);
            Console.WriteLine("Password = " + dian.Password);

            Console.ReadKey();
        }
    }
    public class user
    {
        string username;
        string password;
        public string Username
        {
            get { return username; }
            set { username = value; }
        }

        public string Password
        {
            get { return password; }
            set { password = value; }
        }
    }
}
